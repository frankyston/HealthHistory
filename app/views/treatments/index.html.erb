<p id="notice"><%= notice %></p>

<h1>Treatments</h1>

<table>
  <thead>
    <tr>
      <th>Treatment date</th>
      <th>Treatment location</th>
      <th>Files</th>
      <th>User</th>
      <th>Shared</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treatments.each do |treatment| %>
      <tr>
        <td><%= treatment.treatment_date %></td>
        <td><%= treatment.treatment_location %></td>
        <td><%= treatment.files %></td>
        <td><%= treatment.user_id %></td>
        <td><%= treatment.shared %></td>
        <td><%= link_to 'Show', treatment %></td>
        <td><%= link_to 'Edit', edit_treatment_path(treatment) %></td>
        <td><%= link_to 'Destroy', treatment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Treatment', new_treatment_path %>
